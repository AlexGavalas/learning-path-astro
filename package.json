{
    "name": "learning-path-astro",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "dev": "astro dev",
        "start:server": "node ./dist/server/entry.mjs",
        "build": "astro build",
        "preview": "astro preview",
        "astro": "astro",
        "prettier": "prettier .",
        "check:format": "pnpm prettier --list-different",
        "format": "pnpm prettier --write",
        "lint": "eslint --ignore-path .gitignore .",
        "check:types": "tsc --noEmit",
        "index:notes": "tsm scripts/index-notes.ts",
        "sync:types:supabase": "supabase gen types typescript --project-id cxftalfhgyhwkiwnyqjh --schema public > src/types/database.types.ts && pnpm format",
        "validate": "pnpm lint && pnpm check:format"
    },
    "dependencies": {
        "@astrojs/mdx": "0.19.7",
        "@astrojs/node": "5.2.0",
        "@astrojs/react": "2.2.1",
        "@astrojs/sitemap": "1.3.3",
        "@astrojs/tailwind": "3.1.3",
        "@astrojs/vercel": "3.5.0",
        "@supabase/supabase-js": "2.25.0",
        "@vercel/edge-config": "0.2.1",
        "astro": "2.6.4",
        "react": "18.0.0",
        "react-dom": "18.0.0",
        "tailwindcss": "3.0.24"
    },
    "devDependencies": {
        "@tailwindcss/typography": "0.5.9",
        "@types/react": "18.0.21",
        "@types/react-dom": "18.0.6",
        "@typescript-eslint/eslint-plugin": "5.50.0",
        "@typescript-eslint/parser": "5.59.11",
        "date-fns": "2.30.0",
        "date-fns-tz": "2.0.0",
        "dotenv": "16.3.1",
        "eslint": "8.0.1",
        "eslint-config-prettier": "8.8.0",
        "eslint-config-standard-with-typescript": "35.0.0",
        "eslint-plugin-import": "2.25.2",
        "eslint-plugin-n": "15.0.0",
        "eslint-plugin-promise": "6.0.0",
        "eslint-plugin-react": "7.32.2",
        "gray-matter": "4.0.3",
        "prettier": "2.8.8",
        "react-markdown": "8.0.7",
        "rehype-autolink-headings": "6.1.1",
        "rehype-external-links": "2.1.0",
        "rehype-slug": "5.1.0",
        "supabase": "1.68.6",
        "tsm": "2.3.0",
        "typescript": "5.0.4"
    }
}
